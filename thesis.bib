	Month = {May},
	Number = {5},
	Pages = {564--582},
	Priority = {2},
	Title = {Improving whole-program locality using intra-procedural and inter-procedural transformations,},
	Url = {http://dx.doi.org/10.1016/j.jpdc.2004.12.004},
	Volume = {65},
	Year = {2005},
