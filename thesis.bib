@inproceedings{mishra:01:isss,
	Author = {Prabhat Mishra and Nikil Dutt and Alexandru Nicolau},
	Booktitle = {Proceedings of the } # ISSS,
	Date-Added = {2011-02-17 14:42:11 -0700},
	Date-Modified = {2011-02-17 14:42:19 -0700},
	Month = {October},
	Optannote = {Say the right things about underlying abstractions and reuse of primitives and design space exploration. Uses EXPRESSION (same developers) who claim it can model superscalar architectures. Approach was to look at different architectures and decompose them into little pieces, thus generating a list of primitives. This is just what we would call modules and channels! Control is broken into distributed vs. centralized. Distributed is simply static/dynamic instruction info being carried in pipeline latches. Centralized is a table used for remembering total pipeline state in order to stall. Talk about modeling interrupts and exceptions. Also DMA and external interfaces.},
	Pages = {256-261},
	Title = {Functional Abstraction Driven Design Space Exploration of Heterogeneous Programmable Architectures},
	Url = {citeseer.nj.nec.com/506348.html},
	Year = {2001},
	Bdsk-Url-1 = {citeseer.nj.nec.com/506348.html}}

@inproceedings{onder:98:ICCL,
	Author = {"Soner {\"{O}}nder and Rajiv Gupta},
	Booktitle = {Proceedings of the 1998 } # ICCL,
	Date-Added = {2011-02-17 14:42:11 -0700},
	Date-Modified = {2011-02-17 14:42:19 -0700},
	Month = {May},
	Optannote = {UPFAST - Not a bad idea: there is some attempt to map behavior of instructions to behavior of hardware. However, it is completely unmodular: the description is flat and removing one thing can easily modify others. The user must also figure out how to schedule simulator ops (through minor cycles) if anything other than simple pipe control (extended with stalls, freezes, and reserving of units) is desired. No default behavior of any kind is provided; with a new machine, you start from scratch. Instruction context is rather like dynids, but in the spirit of flatness, you cannot extend it in a scattered fashion: it has to be done in one statement. Regular operations (artifacts) are rather like channels or buildpipe stages. Switching one of these out for a custom one is difficult. ISA must always say how sub-ops map to the uarch. Because all operations must be specified, adding stats is done by changing the semantics of particular stages/instructions, though they can be automagically printed at the end. Note that instructions must follow a regular pipeline structure. How an instruction context is allocated is unclear. Cancelling is done through a retire statement (I think), so you have to be careful about statistics to look at retired instructions retiring in the commit stage. Exceptions are probably doable, and multiple pipelines might manage to do MP (but the absence of modularity and hierarchy would make it hard). Built using templates with inserted code at markers. Descriptions in the 6K-line size. 200Kinst/sec - 100Kinst/sec on 200MHz PentiumII (SS reported 150Kinst/sec at time)},
	Pages = {80--89},
	Title = {Automatic Generation of Microarchitecture Simulators},
	Year = {1998}}

@inproceedings{pees:99:dac,
	Author = {Stefan Pees and Andreas Hoffmann and Vojin {\u{Z}}ivojnovi{\'{c}} and Heinrich Meyr},
	Booktitle = {Proceedings of the } # DAC,
	Date-Added = {2011-02-17 14:42:11 -0700},
	Date-Modified = {2011-02-17 14:42:19 -0700},
	Optannote = {LISA Really, really sketchy description. Too bad they can't say what they did. Nice idea of separating models: memory vs. timing vs. behavioral vs. ISA. Hints at automatically generating docs from description. Some extensibility (user-defined sections). Has multiple pipes, and selection would be through if-then-else. Appears that you have to program arbitration/resource selection yourself. Did these guys come up with compiled-code simulation (seem to claim so)? Claim to be good for superscalar, but no examples.},
	Pages = {933--938},
	Title = {{LISA} -- Machine Description Language for Cycle-Accurate Models of Programmable {DSP} Architectures},
	Year = {1999}}

