@inproceedings{fernandez:88:unknown,
	Author = {Edil S. T. Fernandez},
	Booktitle = {unknown},
	Date-Added = {2011-02-17 14:42:11 -0700},
	Date-Modified = {2011-02-17 14:42:19 -0700},
	Optannote = {Lots of references to ancient HDLs, even back to the 60s. Nice for historical perspective... Very much an RTL-level language. Blocks can be either structural (MACHINE) or functional (ACTION). Aliased registers and subregisters allowed. Derived memory construct allows naming of portions of state. Allows hierarchy, though doesn't seem to have a construct for easily replicating items (could do through preprocessing, I think). Actually constructed a MP-target simulator. They talk about race conditions between statements in the language (identifying that there is a MoC problem without using the term) but leave it to the user to fix them. Some parallel assignments can happen.},
	Title = {Microarchitecture Modeling through ADL},
	Year = {1988}}

@inproceedings{siska:98:isss,
	Author = {C. Siska},
	Booktitle = {Proceedings of the 11th } # ISSS,
	Date-Added = {2011-02-17 14:42:11 -0700},
	Date-Modified = {2011-02-17 14:42:19 -0700},
	Month = dec,
	Optannote = {RADL Starts by mixing LISA and nML types of constructs, I think, but then it does a lot more with the concept of signals and strategies. Strategies are descriptions of pipeline flow control. Signals indicate when a particular strategy applies. For example, the strategy on a RAW hazard may be to stall certain stages. The signal would be asserted when a RAW hazard exists. Signals can be built from boolean expressions of each other. Latches between stages are implied, but their datatypes can be defined. Multiple PC's can be supported by defining pipeline strategies to start instructions in the pipeline (pretty neat idea that!) Hierarchy is posible, as are multiple pipelines talking to each other. Unclear about what the MoC is for these parallel pipelines. In general, good ideas about common flow control issues. Still has an instruction-centric view of what work is to be done in the processor instead of the processor-centric view common to OOO. Could make it difficult to arbitrate between instructions. Extensibility is through reprogramming; definitely not modular. It might be interesting to let buildpipe read RADL to generate simple pipelines.},
	Title = {A processor description language supporting retargetable multi-pipeline {dsp} program development tools},
	Year = {1998}}

