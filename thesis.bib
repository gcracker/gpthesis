@article{emer:02:computer,
	Author = {Joel Emer and Pritpal Ahuja and Eric Borch and Artur Klauser and Chi-Keung Luk and Srilatha Manne and Shubhendu S. Mukherjee and Harish Patil and Steven Wallace and Nathan Binkert and Roger Espasa and Toni Juan},
	Date-Added = {2011-02-17 14:42:11 -0700},
	Date-Modified = {2011-02-17 14:42:17 -0700},
	Journal = {IEEE Computer},
	Month = {February},
	Optannote = {Probably the closest to what we're doing. System is built of modules. Modules are connected through either method calls and ports. Meanings of method calls and ports are by convention among classes of modules to allow module replacement. To make this easier to manage, modules have a "module type" which defines which interface convention (calls and ports) it implements. Modules can also say that they must have some sub-module of a given module type (i.e. a branch predictor must have a branch predictor algorithm sub-module). Feeder modules provide the instructions to execute; can be set up to feed from traces or emulation. Ports model communication delays. MoC isn't clearly specified: each module has a clock method, but how to deal with zero-cycle scheduling isn't clear. Ports are stated to be "good" relative to method calls because they do not have side effects. Strange mechanism for connecting sub-modules to sub-modules without the parent modules knowing what's happening. My guess is that it is similar to the query mechanism. Written in C++; writing new modules means writing new C++ classes. Nice working environment.},
	Pages = {68-76},
	Title = {{Asim}: A Performance Model Framework},
	Volume = {0018-9162},
	Year = {2002}}

